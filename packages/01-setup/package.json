{
  "name": "01-setup",
  "workspaces": {
      "packages": [
          "packages/*"
      ]
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "mdx-code": "^1.1.3",
    "mdx-deck": "^1.7.3",
    "mdx-deck-code-surfer": "^0.5.5",
    "mdx-deck-live-code": "^1.0.0",
    "raw-loader": "^1.0.0"
  },
  "description": "01-setup deck",
  "version": "1.0.0",
  "author": "Sibelius Seraphini",
  "keywords": [],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "mdx-deck build ./deck.mdx",
    "now-build": "mdx-deck build --no-html ./deck.mdx",
    "help": "mdx-deck",
    "image": "mdx-deck screenshot src/deck.mdx",
    "pdf": "mdx-deck pdf src/deck.mdx",
    "copy:static": "cp -r src/img dist",
    "publish:deck": "yarn build && yarn copy:static && gh-pages -d dist",
    "start": "mdx-deck ./deck.mdx"
  },
  "dependencies": {
    "codemirror": "^5.43.0",
    "react-codemirror2": "^5.1.0"
  }
}
